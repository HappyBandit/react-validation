{
  "name": "@happybandit/react-validation",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/HappyBandit/react-validation"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:babel && npm run build:tsc && npm run copy",
    "build:clean": "rimraf ./dist",
    "build:tsc": "tsc -p \"./tsconfig-build.json\"",
    "build:babel": "babel components --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/*.stories.ts,**/*.stories.tsx,**/tests/*\"",
    "copy": "copyfiles \"./components/**/*\" ./dist -e \"./components/**/*.snap\" -e \"./components/**/*.ts\" -e \"./components/**/*.tsx\" -e \"./components/**/*.js\" -e \"./components/**/*.jsx\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "prepublishOnly": "npm test && npm run build",
    "pretest": "npm run test:clean && npm run lint && npm run test:tsc",
    "start": "start-storybook -p 6006",
    "test:clean": "rimraf ./coverage",
    "test": "jest",
    "test:tsc": "tsc",
    "test:watch": "npm run test -- --watchAll",
    "build-storybook": "build-storybook"
  },
  "contributors": [
    {
      "name": "Josh Eckhoff"
    }
  ],
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@sambego/storybook-state": "^2.0.1",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/react": "6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bootstrap": "^5.2.4",
    "@types/jest": "^29.1.0",
    "@types/node": "18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-jest": "^29.1.0",
    "babel-loader": "8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "bootstrap": "5.2.1",
    "copyfiles": "^2.4.1",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.1",
    "jest-environment-jsdom": "29.1.1",
    "react": "^18.2.0",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "typescript": "^4.8.4"
  }
}
